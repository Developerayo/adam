{
	"name": "adam",
	"version": "0.1.0",
	"description": "Adam is a CLI tool designed to help developers generate/execute simple and complex shell commands. Smart enough to understand your project's structure and suggest the right commands based on what you're working on.",
	"main": "dist/index.js",
	"bin": {
		"adam": "./dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.ts",
		"watch": "tsc --watch",
		"postinstall": "node -e \"console.log('Run npm run build to compile TypeScript')\"",
		"postbuild": "node --input-type=module -e \"import { ascii } from './dist/utils/ascii.js'; console.log(ascii)\"",
		"test": "tsx src/index.test.ts",
		"start": "node dist/index.js",
		"prepublishOnly": "npm run build"
	},
	"keywords": [],
	"contributors": [
		{
			"name": "Shodipo Ayomide",
			"email": "shodipovi@gmail.com"
		},
		{
			"name": "Emeka Orji",
			"email": "emekapraiseo@gmail.com"
		}
	],
	"license": "MIT",
	"dependencies": {
		"@google-cloud/speech": "^6.7.0",
		"@google/generative-ai": "^0.21.0",
		"@iarna/toml": "^2.2.5",
		"assemblyai": "^4.6.1",
		"chalk": "^5.3.0",
		"dotenv": "^16.4.5",
		"enquirer": "^2.4.1",
		"js-yaml": "^4.1.0",
		"node-fetch": "^2.7.0",
		"node-localstorage": "^3.0.5",
		"node-record-lpcm16": "^1.0.1",
		"openai": "^4.55.7",
		"ora": "^8.0.1",
		"semver": "^7.6.3",
		"xml2js": "^0.6.2",
		"yargs": "^17.7.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.7",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.11.0",
		"@types/node-fetch": "^2.6.11",
		"@types/semver": "^7.5.6",
		"@types/xml2js": "^0.4.14",
		"@types/yargs": "^17.0.32",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
